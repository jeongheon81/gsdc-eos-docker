

/var/lib/docker/volumes/eos-mgm-01_log/_data/*/*.log
/var/lib/docker/volumes/eos-mgm-01_log/_data/tx/*.log
/var/lib/docker/volumes/eos-mgm-01_log/_data/mgm/xrdlog.mgm
/var/lib/docker/volumes/eos-mgm-01_log/_data/mgm/*.log
{
        createolddir 755 daemon daemon
        olddir archive
        notifempty
        missingok
        daily
        copytruncate
        create 644 daemon daemon
        dateext
        rotate 200
        compress
        delaycompress
}

